digraph {
    rankdir=LR
    Start[class="extremity"]
    End[class="extremity"]
    A[class="start,disabled"]
    B[style="invis"]
    ChainB[label="Chain[B]" class="goal"]

    A -> B[label="f" style="invis"]
    B -> ChainB[label="Done" style="invis"]

    Start -> A[class="start,disabled"]
    ChainB -> End[class="goal"]
}
